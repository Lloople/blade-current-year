{
    "name": "lloople/blade-current-year",
    "description": "Display the current year in your blade views",
    "keywords": [
        "lloople",
        "blade-current-year"
    ],
    "homepage": "https://github.com/lloople/blade-current-year",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Llop",
            "email": "d.lloople@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lloople\\BladeCurrentYear\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lloople\\BladeCurrentYear\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lloople\\BladeCurrentYear\\BladeCurrentYearServiceProvider"
            ],
            "aliases": {
                "BladeCurrentYear": "Lloople\\BladeCurrentYear\\BladeCurrentYearFacade"
            }
        }
    }
}
